<h1>Sign Up Confirmation</h1>
<h3>Are you sure these values are correct?</h3>
<h4>Email: <%= user_params[:email] %> </h4>
<h4>Name: <%= user_params[:name] %> </h4>

<%= button_to "Yes", { controller: "users", action: "create", params: { user: user_params } }, class: 'btn btn-success pull-left' %>
<%= link_to "No", { controller: "users", action: "new" }, class: 'btn btn-info' %>
